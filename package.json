{
  "name": "nodeapptemplate",
  "version": "1.0.0",
  "description": "This repo serves as template to build functioning, deployable node js apps running on express. ",
  "main": "index.js",
  "scripts": {
    "build": "tsc",
    "pretest": "",
    "prepublish": "prettier --check .",
